		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "^9.6.1",
		"@types/toastify-js": "^1.12.3",
		"apexcharts": "^3.54.0",
		"autoprefixer": "^10.4.20",
		"dayjs": "^1.11.13",
		"eslint": "^9.11.1",
		"eslint-config-prettier": "^9.1.0",
